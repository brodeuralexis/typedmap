{
  "name": "typedmap",
  "version": "1.0.0",
  "description": "A dynamically typed map in TypeScript",
  "main": "lib/index.js",
  "repository": "github.com/brodeuralexis/typemap",
  "author": "Alexis Brodeur <brodeuralexis@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "yarn tsc",
    "lint": "yarn tslint --project .",
    "test": "yarn jest"
  },
  "devDependencies": {
    "@types/jest": "^23.0.0",
    "jest": "^22.0.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-standard": "^7.0.0",
    "typescript": "^2.9.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
